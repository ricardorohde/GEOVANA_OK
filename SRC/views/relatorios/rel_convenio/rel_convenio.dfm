  object sql_conveniados: TFDQuery
    Connection = Module.connection
    SQL.Strings = (
      'select 0 as id, '#39'Selecionar todos os conveniados.'#39' as conveniado'
      'union all '
      
        'select cc.id, concat(cc.nome, '#39' (CPF: '#39', cc.cpf, '#39' - Matr'#237'cula: ' +
        #39', cc.matricula, '#39')'#39') as conveniado from cliente_conveniado cc'
      'where cc.cliente_id = :pcliente_id')
    Left = 208
    Top = 72
    ParamData = <
      item
        Name = 'PCLIENTE_ID'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
